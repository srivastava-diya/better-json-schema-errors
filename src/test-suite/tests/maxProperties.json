{
  "$schema": "../test-suite.schema.json",

  "description": "The maxProperties keyword",
  "tests": [
    {
      "description": "maxProperties",
      "schema": {
        "maxProperties": 1
      },
      "instance": { "foo": 1, "bar": 2 },
      "errors": [
        {
          "messageId": "maxProperties-message",
          "messageParams": {
            "maxProperties": "1"
          },
          "instanceLocation": "#",
          "schemaLocations": ["#/maxProperties"]
        }
      ]
    },
    {
      "description": "maxProperties pass",
      "schema": {
        "maxProperties": 1
      },
      "instance": { "foo": 1 },
      "errors": []
    },
    {
      "description": "multiple maxProperties constraints (collapsed)",
      "schema": {
        "allOf": [
          { "maxProperties": 2 },
          { "maxProperties": 3 }
        ]
      },
      "instance": { "a": 1, "b": 2, "c": 3, "d":4 },
      "errors": [
        {
          "messageId": "maxProperties-message",
          "messageParams": {
            "maxProperties": "2"
          },
          "instanceLocation": "#",
          "schemaLocations": ["#/allOf/0/maxProperties"]
        }
      ]
    }
  ]
}
